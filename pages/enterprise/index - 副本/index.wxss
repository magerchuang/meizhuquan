.checkbox{
width:30.6%;
height: 62rpx;
line-height: 62rpx;
font-size: 23rpx;
color: #888888;
display: inline-block;
margin: 0 10rpx 20rpx 0;
border-radius: 30rpx;
position: relative;
text-align:center;
background-color: #f5f5f5;
border: 1rpx solid #f5f5f5;
}
.checkbox:nth-of-type(3n){
  margin-right: 0;
}
.checkbox checkbox{
 display: none
}
.checked{
