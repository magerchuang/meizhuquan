/* 送券按钮 */
.radio {  position: absolute;  top: 35%;  left: 0%;}
.radio .radio_checked{
   border-radius: 50%;/* 圆角 */
   width: 35rpx; /* 背景的宽 */
   height: 35rpx; /* 背景的高 */
   border:1rpx solid #ff498f;
   box-sizing: border-box;
}
/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */
.radio .radio_checked.checked::before{
   border-radius: 50%;/* 圆角 */
   width: 15rpx;/* 选中后对勾大小，不要超过背景的尺寸 */
   height: 15rpx;/* 选中后对勾大小，不要超过背景的尺寸 */
   line-height: 35rpx;
   content:'';
   background:#ff006e; /* 对勾颜色 白色 */
   position:absolute;
   transform: translate(-50%, -50%);
   -webkit-transform: translate(-50%, -50%);
   left:50%;
   top:50%;
}

