page{height:100%;width:100%;background: #fff;}
.container{padding: 0 30rpx;}

.container{height:100%;}

/*简历*/
.resume{height:calc(100% - 100rpx);overflow:hidden;box-sizing: border-box;width:100%;}
/* .resume_con{padding:0 30rpx 20rpx;} */
.info_card{padding:20rpx;background: #fff;position: relative;overflow: hidden;}
.info_card .tit{font-size:34rpx;}


.mt15{margin-top:15rpx;}
.mt20{margin-top:20rpx;}
.float,.layer {height: 100%;width: 100%;position: fixed;background-color: rgba(0, 0, 0, 0.5);z-index: 2;top: 0;left: 100%;transition: 0.3s;z-index:999999;}
.maskShow{left:0;}
.position{width: 80%;height: 100vh;background-color: #ffffff; position: absolute;right:0;}
.trade,.position_all{height: 100%;width:100%;}
.trade_hd{font-size:34rpx;color:#000;height:100rpx;line-height: 100rpx;padding: 0 20rpx;}
.trade_bd{display: flex;flex-wrap: wrap;padding:0 20rpx;}
.trade_bd .list_box{width:31.33%;text-align: center;background: #f2f2f2;margin:0 1% 15rpx;height: 60rpx;font-size: 22rpx;line-height: 60rpx;border-radius: 35rpx;color:#666;box-sizing: border-box;border:1rpx solid #f2f2f2;}
.trade_bd .list_box.on{border:1rpx solid #ff498f;color:#ff498f;}
.buttom,.button{position: fixed;bottom: 0;display: flex;display: -webkit-flex;width:80%;font-size:34rpx;}
.animation-reset{flex: 1;line-height: 3;width: 50%;display: block;text-align: center;background: #f2f2f2;}
.animation-button{flex: 1;line-height: 3;width: 50%;display: block;text-align: center;background: #ff498f;color:#fff;}
.black{color:#000}
.position_hd{padding:20rpx 20rpx;font-size:36rpx;}
.position_bd{padding:0 20rpx;}
.position_tit{font-size:30rpx;padding: 10rpx 0;}
.position_tit text:nth-child(2){position: relative;color:#999;font-size:26rpx;float: right;padding-right:30rpx;}
.position_tit text:nth-child(2)::after{ border-right: 1rpx solid #999;border-bottom: 1rpx solid #999;content: "";position: absolute;width: 12rpx;height:12rpx;right:0;top:8rpx;font-size:20rpx;transform: rotate(45deg);}
.position_con{display: flex;width:100%;display: -webkit-flex;flex-wrap: wrap;}
.p_list{width:31%;margin:10rpx 1% 5rpx;font-size:24rpx;background: #f2f2f2;border-radius: 40rpx;height: 60rpx;line-height: 60rpx;text-align: center;}
.btn button{background: #ff498f;color:#fff;}

/* 公司介绍 */
.video{width:100%;height: 100%;position: relative;}
.photo{position: absolute;bottom:30rpx;width:100%;text-align: center;}
.takePhoto{width:150rpx;height: 150rpx;border-radius: 100%;border:10rpx solid #ff498f;padding: 10rpx;margin:0 auto;opacity: 0.6;}
.takePhoto button{width:150rpx;height:150rpx;border-radius: 100%;background: #ff498f;display: block;color:#fff;line-height: 150rpx;text-align: center;color:#fff;font-size:28rpx;}
.takePhoto_txt{color:#fff;font-size:28rpx;text-align: center;padding-top: 10rpx;}
.gw{color:#666;font-size:32rpx;}
.selectGw{position: relative;border-bottom:1rpx solid #f2f2f2;}
.selectGw::after{content: " ";
    display: inline-block;
    height: 12rpx;
    width: 12rpx;
    border-width: 4rpx 4rpx 0 0;
    border-color: #e6e6e6;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    top: -2px;
    position: absolute;
    top: 50%;
    margin-top: -4px;
    right: 2px;}
.post{display: flex;}
.post text{display: inline-block;line-height: 100rpx;}
.post text{width:150rpx;}
.post view{width:560rpx;display: inline-block;padding-right:40rpx;text-align: right;box-sizing:border-box;}
.gw .selectXz{display: flex;position: relative;border-bottom:1rpx solid #f2f2f2;}
.gw .selectXz::after{content: " ";
    display: inline-block;
    height: 16rpx;
    width: 16rpx;
    border-width: 4rpx 4rpx 0 0;
    border-color: #e6e6e6;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    top: -2px;
    position: absolute;
    top: 50%;
    margin-top: -4px;
    right: 2px;}
.gw .selectXz text{width:150rpx;display: inline-block;line-height: 100rpx;}
.gw .selectXz .picker{display: inline-block;width:560rpx;line-height: 100rpx;color:#ff498f;padding-right:40rpx;text-align: right;box-sizing:border-box;}





/*企业位置  经验要求 学历*/
.flag{font-size: 22rpx;}
.flag_item{color:#999;display: inline-block;padding:10rpx 40rpx;margin-right:20rpx;background: #e5e5e5;border-radius: 8rpx;}
.c_box .c_jn,.c_box .c_ms{margin-top:20rpx;}
.c_tit{font-size:30rpx;}
.c_bd{font-size: 26rpx;color:#999;margin-top:20rpx;}
.c_bd textarea{width:100%;height: 150rpx;box-sizing: border-box;border:1rpx solid #f2f2f2;padding:20rpx;border-radius: 8rpx;line-height: 30rpx;font-size: 24rpx;}
.c_box .textarea_layer{width:100%;height: 150rpx;box-sizing: border-box;border:1rpx solid #f2f2f2;padding:20rpx;border-radius: 8rpx;display: block;line-height: 30rpx;font-size: 24rpx;color:#999;}
.textareaplaceholer{line-height: 30rpx;font-size: 24rpx;color:#999;}
.cell{position: relative;display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-align: center;-webkit-align-items:center;align-items:center;border-bottom:1rpx solid #e6e6e6;}
.cell text{display: block;width:200rpx;word-break: break-all;word-wrap: break-word;font-size:30rpx;padding:30rpx 0;}
.cell_bd{-webkit-box-flex:1;-webkit-flex:1;flex: 1;text-align: right;height: 100rpx;padding-right:40rpx;box-sizing: border-box;line-height: 100rpx;font-size: 30rpx;}
.cell_bd input{-webkit-tap-highlight-color: rgba(0, 0, 0, 0);width:100%;border:0;outline: 0;-webkit-appearance: none;background: transparent;font-size:30rpx;text-align: right;padding:30rpx 0;}

.picker{font-size:30rpx;color:#666;text-align: right;}
.section::after{content: " ";
    display: inline-block;
    height: 12rpx;
    width: 12rpx;
    border-width: 4rpx 4rpx 0 0;
    border-color: #e6e6e6;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    top: -2px;
    position: absolute;
    top: 50%;
    margin-top: -4px;
    right: 2px;}
/*新增职位信息*/
.add_card{padding:50rpx 0;background: #fff;width:100%;}
.add_box{width:50rpx;height:50rpx;border:6rpx solid #999;border-radius: 50%;margin:0 auto;position: relative;}
.add_box::before{content: "";width:30rpx;height:6rpx;background: #999;position: absolute;left:50%;top:50%;transform: translate(-50%,-50%)}
.add_box::after{content: "";width:30rpx;height:6rpx;background: #999;position: absolute;left:50%;top:50%;transform: translate(-50%,-50%) rotate(90deg);}
.subBtn{margin-top:30rpx;}
.subBtn button{background: #ff498f;}
.edit{position: absolute;right:30rpx;display: inline-block;padding:5rpx 30rpx;font-size: 28rpx;color:#fff;background: #ff498f;border-radius: 8rpx;top:20rpx;}
