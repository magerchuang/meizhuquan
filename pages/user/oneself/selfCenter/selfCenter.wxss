/* pages/user/selfCenter/selfCenter.wxss */
page{height:100%;width:100%;background: #f2f2f2;}
.container{height:100%;}
.toptab{width:100%;display: flex;background: #fff;align-items: center;height: 99rpx;border-bottom:1rpx solid #f2f2f2;}
.toptab_item{flex: 1;text-align: center;border-bottom:4rpx solid #fff;}
.toptab_item image{width:44rpx;height:44rpx;}
.toptab_item .item_txt{font-size: 34rpx;color:#666;line-height: 95rpx;}
.toptab_item.on{border-bottom:5rpx solid #ff006e;}
.toptab_item.on .item_txt{color:#ff006e;}
.card{}
.card .updata{width:140rpx;height:140rpx;margin-right:20rpx;border:1rpx solid #D9D9D9;position: relative;float:left;}
.card .updata::before,.card .updata::after{content: '';position:absolute;top:50%;left:50%;    -webkit-transform: translate(-50%, -50%);transform: translate(-50%, -50%);background-color: #D9D9D9;}
.card .updata::before{width:2rpx;height:30rpx;}
.card .updata::after{width:30rpx;height:2rpx;}
.btn_area button{background: #ff006e;color:#fff;}
.qiye{height:calc(100% - 100rpx);width:100%;overflow-y: auto;box-sizing: border-box;}
.nothing{padding-top:300rpx;text-align: center;}
.nothing image{width:126rpx;height:96rpx;margin-bottom:20rpx;}
.nothing text{display: block;text-align: center;font-size:28rpx;color:#999;}
/*已购企业*/
.enterprise{padding:0 0 20rpx;}
.qiyelist{margin-top:20rpx;background:#fff;width:100%;border-radius: 10rpx;box-sizing: border-box;padding:20rpx 30rpx;display: flex;align-items: center;justify-content: space-around;}
.qiye_name{text-align: left;flex: 1;}
.qiye_name image{height:80rpx;width:80rpx;vertical-align: middle;margin-right:20rpx;}
.qiye_name text{display: inline-block;font-size:30rpx;}
.qy_back{font-size:24rpx;color:#ff498f;text-align: right;}
/*简历*/
.resume{height:calc(100% - 100rpx);overflow:hidden;box-sizing: border-box;width:100%;}
/* .resume_con{padding:0 30rpx 20rpx;} */
.info_card{padding:20rpx;background: #fff;border-radius: 10rpx;margin-top:20rpx;}
.info_cardtit{border-bottom:1rpx solid #e6e6e6;position: relative;padding-bottom:20rpx;}
.info_cardtit image{width:32rpx;height:32rpx;vertical-align: middle;margin-right:20rpx;}
.info_cardtit text{font-size:32rpx;color:#333;}
